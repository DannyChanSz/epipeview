@model IEnumerable<NetPress.Models.CartToDestroy>

@{
    ViewBag.Title = "Index";
}

<h2>列表</h2>

<p>
    @Html.ActionLink("新建", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Customer.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PriceId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PackType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PurchaseVolume)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cause)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Customer.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PriceId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PackType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PurchaseVolume)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cause)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CartToDestroyId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CartToDestroyId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CartToDestroyId })
        </td>
    </tr>
}

</table>
