@model IEnumerable<NetPress.Models.Coupon>

@{
    ViewBag.Title = "Index";
}

<h2>列表</h2>

<p>
    @Html.ActionLink("新建", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Customer.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Denomination)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CouponType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OffTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Customer.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Denomination)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CouponType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OffTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CouponId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CouponId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CouponId })
        </td>
    </tr>
}

</table>
